        <!-- Masonry Script, by DeSandro -->
        <script type="text/javascript" charset="utf-8">$(document).ready(function(){
        var $grid = $("body").masonry({
             itemSelector: "article",
             initLayout: 1
            });
            $grid.imagesLoaded().progress(function() {
             $grid.masonry("layout")
            });
            });
        </script>
