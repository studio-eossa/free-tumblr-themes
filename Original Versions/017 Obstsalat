                    <div class="text">{Body}</div>
                    {/block:Reblogs}
                    {/block:RebloggedFrom}
                    {/block:PermalinkPage}
                {/block:IfNotShowCaptions} <!-- / end audio post -->
                {/block:Caption}
                {/block:Audio}

