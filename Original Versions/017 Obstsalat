    <script>
        $(document).ready(function() {
            $('iframe.tumblr_audio_player').load( function() {
                $('iframe.tumblr_audio_player').contents().find("head")
                    .append($("<style type='text/css'> .audio-player{background:{color:Audio Background} !important} </style>"));
                 $('iframe.tumblr_audio_player').contents().find("head")
                    .append($("<style type='text/css'> .track-name{color:{color:Audio Text} !important} </style>"));
                 $('iframe.tumblr_audio_player').contents().find("head")
                    .append($("<style type='text/css'> .track-artist{color:{color:Audio Text} !important} </style>"));
                 $('iframe.tumblr_audio_player').contents().find("head")
                    .append($("<style type='text/css'> .play-pause{color:{color:Audio Text} !important} </style>"));
            });
        });
    </script>

