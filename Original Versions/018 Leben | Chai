    <script>
        $(document).ready(function() {
            $('iframe.tumblr_audio_player').load( function() {
                $('iframe.tumblr_audio_player').contents().find("head")
                    .append($("<style type='text/css'> .audio-player{background:{color:Accent 7} !important} </style>"));
                 $('iframe.tumblr_audio_player').contents().find("head")
                    .append($("<style type='text/css'> .track-name{color:{color:Accent 2} !important} </style>"));
                 $('iframe.tumblr_audio_player').contents().find("head")
                    .append($("<style type='text/css'> .track-artist{color:{color:Accent 2} !important} </style>")); 
                 $('iframe.tumblr_audio_player').contents().find("head")
                    .append($("<style type='text/css'> .play-pause{color:{color:Accent 2} !important} </style>"));    
            });
        });
    </script>
    
